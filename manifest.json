{
  "name": "comfynet",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "comfynet.",
  "icons": {
    "16": "icons/solar16.png",
    "32": "icons/solar32.png",
    "48": "icons/solar48.png",
    "128": "icons/solar128.png"
  },
  "chrome_url_overrides": {
    "newtab": "ui/new_tab.html"
  },
  "browser_action": {
    "default_popup": "ui/popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "https://*/",
    "http://*/"
  ],
  "web_accessible_resources": [
    "images/*.png",
    "ui/*",
    "scr/*",
    "build/*"
  ]
}
